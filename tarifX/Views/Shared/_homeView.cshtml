
@{
    Layout = null;
}

<!DOCTYPE html>
<html lang="en">
<head>
    <title>Food Blog - TarifX</title>
    <meta charset="UTF-8">
    <meta name="description" content="Food Blog Web Template">
    <meta name="keywords" content="food, unica, creative, html">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <!-- Favicon -->
    <link href="~/assets/Home/img/favicon.ico" rel="shortcut icon" />

    <!-- Google Fonts -->
    <link href="~/assets/Home/https://fonts.googleapis.com/css?family=Poppins:400,400i,500,500i,600,600i,700" rel="stylesheet">

    <!-- Stylesheets -->
    <link rel="stylesheet" href="~/assets/Home/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/assets/Home/css/font-awesome.min.css" />
    <link rel="stylesheet" href="~/assets/Home/css/owl.carousel.css" />
    <link rel="stylesheet" href="~/assets/Home/css/animate.css" />
    <link rel="stylesheet" href="~/assets/Home/css/style.css" />


    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <script src="//cdn.ckeditor.com/4.10.1/standard/ckeditor.js"></script>

    <script src='https://www.google.com/recaptcha/api.js'></script>
</head>
<body>
    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <!-- Header section -->
    <header class="header-section">
        <div class="header-top">
            <div class="container">
                <div class="header-social">
                    <a href="#"><i class="fa fa-pinterest"></i></a>
                    <a href="#"><i class="fa fa-facebook"></i></a>
                    <a href="#"><i class="fa fa-twitter"></i></a>
                    <a href="#"><i class="fa fa-dribbble"></i></a>
                    <a href="#"><i class="fa fa-behance"></i></a>
                    <a href="#"><i class="fa fa-linkedin"></i></a>
                </div>
                <div class="user-panel">
                    <a href="/AdminYemek/Index">Admin Giriş</a>
                </div>
            </div>
        </div>
        <div class="header-bottom">
            <div class="container">
                <a href="/Home/Index" class="site-logo">
                    <img src="~/assets/Home/img/logo.png" alt="">
                </a>
                <div class="nav-switch">
                    <i class="fa fa-bars"></i>
                </div>
                <div class="header-search">
                    <a href="#"><i class="fa fa-search"></i></a>
                </div>
                <ul class="main-menu">
                    <li><a href="/Home/Index">Anasayfa</a></li>
                    <li><a href="/Home/About">Hakkımızda</a></li>
                    <li><a href="/Recipe/Index">Yemek Tarifleri</a></li>
                    <li><a href="/Recipe/SendFood">Tarif Gönder</a></li>
                    <li><a href="/Contact/Index">İletişim</a></li>
                </ul>
            </div>
        </div>
    </header>
    <!-- Header section end -->
        @RenderBody()
    <!-- Gallery section -->
    <div class="gallery">
        <div class="gallery-slider owl-carousel">
            <div class="gs-item set-bg" data-setbg="/assets/Home/img/instagram/1.jpg"></div>
            <div class="gs-item set-bg" data-setbg="/assets/Home/img/instagram/2.jpg"></div>
            <div class="gs-item set-bg" data-setbg="/assets/Home/img/instagram/3.jpg"></div>
            <div class="gs-item set-bg" data-setbg="/assets/Home/img/instagram/4.jpg"></div>
            <div class="gs-item set-bg" data-setbg="/assets/Home/img/instagram/5.jpg"></div>
            <div class="gs-item set-bg" data-setbg="/assets/Home/img/instagram/6.jpg"></div>
        </div>
    </div>
    <!-- Gallery section end -->
    <!-- Footer section  -->
    <footer class="footer-section set-bg" data-setbg="/assets/Home/img/footer-bg.jpg">
        <div class="container-fluid">
            <div class="row">
                <div class="col-lg-6">
                    <div class="footer-logo">
                        <img src="~/assets/Home/img/logo.png" alt="">
                    </div>
                    <div class="footer-social">
                        <a href="#"><i class="fa fa-pinterest"></i></a>
                        <a href="#"><i class="fa fa-facebook"></i></a>
                        <a href="#"><i class="fa fa-twitter"></i></a>
                        <a href="#"><i class="fa fa-dribbble"></i></a>
                        <a href="#"><i class="fa fa-behance"></i></a>
                        <a href="#"><i class="fa fa-linkedin"></i></a>
                    </div>
                </div>
                <div class="col-lg-6 text-lg-right">
                    <ul class="footer-menu">
                        <li><a href="/Home/Index">Anasayfa</a></li>
                        <li><a href="/Home/About">Hakkımızda</a></li>
                        <li><a href="/Recipe/Index">Yemek Tarifleri</a></li>
                        <li><a href="/Recipe/SendFood">Tarif Gönder</a></li>
                        <li><a href="/Contact/Index">İletişim</a></li>
                    </ul>
                    <p class="copyright">
                       
                        Copyright &copy;
                        <script>document.write(new Date().getFullYear());</script> All rights reserved | TarifX | Tolgahan KİŞİN
                        
                    </p>
                </div>
            </div>
        </div>
    </footer>
    <!-- Footer section end -->
    <!--====== Javascripts & Jquery ======-->
    <script src="~/assets/Home/js/jquery-3.2.1.min.js"></script>
    <script src="~/assets/Home/js/owl.carousel.min.js"></script>
    <script src="~/assets/Home/js/main.js"></script>
</body>
</html>